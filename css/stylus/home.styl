h1#site_title {
  color: white;
  mix-blend-mode: overlay;
}
h2#site_slogan {
  color: white;
}
#scroll_down_button {
  background-color: none;
  color: color_scrolldown_button;
  setLinearTransition(background-color, 200ms)
}
