@media only screen {
  /* General Styles */
  html {
    width: 100%;
    height: 100%;
    margin: 0;
    padding: 0;
  }
  body {
    -webkit-font-smoothing: antialiased;
    text-rendering: optimizeLegibility;
    -webkit-text-size-adjust: none;
    font-family: font_body, serif;
    font-size: 16px;
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    width: 100%;
    height: 100%;
    line-height: 28px;
    letter-spacing: 0;
    margin: 0;
    padding: 0;
    position: relative;
  }
  h1, h2, h3, h4, h5, h6 {
    margin-top: 0;
    margin-bottom: 22px;
    font-family: font_logo, sans-serif;
  }
  h1 {
    font-size: 44px;
    line-height: 60px;
    letter-spacing: -2px;
  }
  h2 {
    font-size: 38px;
    line-height: 54px;
    letter-spacing: -2px;
  }
  h3 {
    font-size: 32px;
    line-height: 48px;
    letter-spacing: -1px;
  }
  h4 {
    font-size: 26px;
    line-height: 42px;
    letter-spacing: -1px;
  }
  h5 {
    font-size: 20px;
    line-height: 36px;
    letter-spacing: 0px;
  }
  h6 {
    font-size: 18px;
    line-height: 34px;
    letter-spacing: 0px;
  }
  p {
    margin-top: 0;
  }
  b, strong {
  }
  a:link, a:visited {
  }
  a:hover, a:active {
  }
  .image-left {
    float: left;
    margin-right: 20px;
  }
  .image-right {
    float: right;
    margin-left: 20px;
  }
  .clearer {
    clear: both;
  }
  blockquote, .standout {
    font-size: 24px;
    line-height: 1.4;
    font-style: italic;
  }
  #tools {
    
  }
  .imageContainer {
    text-align: center;
  }
  .avatar {
    display: inline-block;
    width: 128px;
    height: 128px;
    border-radius: 50%;    
    background-size: cover;
    background-position-x: center;
    margin: 20px;
    margin-left: 40px;
  }
  .avatar_title {
    font-family: font_logo, sans-serif;
    font-size: 22px;
    font-weight: 600;
  }
  .avatar_item {
    display: inline-block;
    font-family: font_logo, sans-serif;
  }
  .roundImage {
    position: relative;
    display: inline-block;
    width: 256px;
    height: 256px;
    line-height: 256px;
    border-radius: 50%;
    margin: 15px;
    color: white;
    font-family: font_logo, sans-serif;
    font-size: 36px;
    background-color: color_image_blend;
    background-blend-mode: screen;
    background-size: cover;
    background-position-x: center;
    setTransition(background-color, .25s)
  }
  .roundImage:hover {
    background-color: black;
    color: transparent;
    setTransition(background-color, .5s)
  }
  figure {
    margin: 22px 0;
  }
  
  /* ----------------------------------------- */
  /* Layout */
  /* ----------------------------------------- */
  /* Banner Section */
  header {
    background-repeat: no-repeat;
    background-position: center center;
    background-size: cover;
    background-cover: none;
    height: 100%;
    width: auto;
    position: relative;
    text-align: center;
  }
  #logo, #title_wrapper {
    -webkit-transform-style: preserve-3d;
    display: block;
    height: 50%;
    width: 100%;
    position: absolute;
    text-align: center;
  }
  #logo img {
    position: relative;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
    max-height: 90%;
    width: auto;
  }
  #title_wrapper {
    top: 50%;
  }
  #brand, #mobile_brand {
    position: absolute;
    width: 100px;
    height: 70px;
    text-align: center;
    font-family: font_logo, sans-serif;
    display: block;
  }
  #navigation_bar #brand {
    color: white;
    padding-left: 20px;    
    padding-top: 19px;
    font-size: 32px;
  }
  #mobile_brand {
    width: 100%;
    padding-top: 30px;
    font-size: 50px;
    display: none;
  }
  #lang {
    position: absolute;
    width: 100%;
    text-align: right;
    padding: 20px;
    font-family: font_logo, sans-serif;
    font-weight: 600;
    font-size: 12px;
  }
  #lang a, #lang a:visited {
    color: color_button;
    border-style: solid;
    border-radius: 5px;
    border-width: 1px;
    padding: 10px;
    text-decoration: none;
    background-color: none;
    setLinearTransition(background-color, 200ms)
  }
  #lang a:hover, #lang a:active {
    color: white;
    border-color: color_hover_button;
    background-color: color_hover_button;
    setLinearTransition(background-color, 200ms)
  }
  #signature {
    display: inline-block;
    width: 231px;
    height: 84px;
    background-size: cover;
    background-position-x: center;
    background-image: url(../assets/signature.png);
  }
  h1#site_title {
    display: inline-block;
    margin: 0;
    padding: 4px 16px;
    font-family: font_logo, sans-serif;
    font-size: 100px;
    line-height: 76px;
    position: relative;
    font-weight: 300;
    mix-blend-mode: overlay;
  }
  h2#site_slogan {
    display: block;
    margin: 0;
    padding: 4px 16px;
    font-family: font_logo, serif;
    font-size: 20px;
    font-weight: 300;
    position: relative;
    letter-spacing: 2px;
  }
  #scroll_down_button {
    display: block;
    width: 40px;
    height: 40px;
    border-radius: 60px;
    font-size: 16px;
    line-height: 40px;
    text-align: center;
    margin: 30px auto 0 auto;
    cursor: pointer;
    border: color_scrolldown_button solid 1px;
    position: relative;
  }
  #scroll_up_button {
    display: block;
    width: 40px;
    height: 40px;
    border-radius: 60px;
    font-size: 16px;
    line-height: 39px;
    text-align: center;
    cursor: pointer;
    position: fixed;
    bottom: 40px;
    right: 50%;
    margin-right: -20px;
    display: none;
    opacity: 0.9;
    box-shadow: 0 0 8px color_scroll_button;
  }
